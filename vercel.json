{
  "version": 2,
  "builds": [
    {
      "src": "index.js",
      "use": "@vercel/node",
      "config": {
        "maxLambdaSize": "15mb",
        "includeFiles": [
          "node_modules/chrome-aws-lambda/**",
          "node_modules/puppeteer-core/**"
        ]
      }
    }
  ],
  "routes": [
    {
      "src": "/keepalive",
      "dest": "/index.js"
    },
    {
      "src": "/",
      "dest": "/index.js"
    }
  ]
}
